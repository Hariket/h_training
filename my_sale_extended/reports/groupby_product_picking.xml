<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_groupby_product_picking">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Requirement 23 and 24</h2>
                        <table class="table table-sm">
                            <thead>
                                <tr>
                                    <tr>
                                        <th>Product</th>
                                    </tr>
                                    <tr>
                                        <th>Warehouse</th>
                                        <th>Qty</th>
                                        <th>Total</th>
                                    </tr>
                                </tr>
                            </thead>
                            <tbody>
                                <tr t-foreach="line" t-as="l">
                                    <tr>
                                        <td>
                                            <strong>
                                                <span t-esc="l.name"/>
                                            </strong>
                                        </td>
                                    </tr>
                                    <tr>
                                        <t t-foreach="line.get(l)" t-as="warehouse_qty">
                                            <tr>
                                                <td>
                                                    <span t-esc="list(warehouse_qty.keys())[0]"/>
                                                </td>
                                                <td>
                                                    <span t-esc="list(warehouse_qty.values())[0]"/>
                                                </td>
                                            </tr>
                                        </t>
                                    </tr>
                                    <tr>
                                        <td>
                                            <strong>
                                                <span>Total</span>
                                            </strong>
                                        </td>
                                        <td class="text-right">
                                            <span t-esc="sum(list(map(lambda x:list(x.values())[0],line.get(l))))"/>
                                        </td>
                                    </tr>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>