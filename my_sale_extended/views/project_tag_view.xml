        <field name="inherit_id" ref="project.project_tags_tree_view"></field>
        <field name="model">project.tags</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="users_toTags_ids" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>
</odoo>
